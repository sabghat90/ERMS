<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/reg_nav_graph"
    app:startDestination="@id/managerLoginFragment">

    <fragment
        android:id="@+id/managerLoginFragment"
        android:name="com.kust.ermsmanager.ui.auth.ManagerLoginFragment"
        android:label="fragment_manager_login"
        tools:layout="@layout/fragment_manager_login" >
        <action
            android:id="@+id/action_managerLoginFragment_to_managerSignUpFragment"
            app:destination="@id/managerSignUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/managerSignUpFragment"
        android:name="com.kust.ermsmanager.ui.auth.ManagerSignUpFragment"
        android:label="fragment_manager_sign_up"
        tools:layout="@layout/fragment_manager_sign_up" />
</navigation>